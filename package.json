{
  "name": "@vidispine/admin-tool",
  "homepage": "https://vidispine.github.io/admin-tool",
  "version": "5.0.0-pre.1",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@vidispine/vdt-api": "^0.14.0",
    "clsx": "^1.1.1",
    "codemirror": "^5.60.0",
    "history": "^5.0.0",
    "immutability-helper": "^3.1.1",
    "lodash.startcase": "^4.4.0",
    "material-ui-chip-input": "1.1.0",
    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-codemirror": "1.0.0",
    "react-cookie": "^4.0.3",
    "react-dom": "^17.0.2",
    "react-json-view": "^1.21.3",
    "react-redux": "^7.2.3",
    "react-router-dom": "5.2.0",
    "react-scripts": "^4.0.3",
    "react-select": "^4.3.0",
    "redux": "^4.0.5",
    "redux-form": "^8.3.7"
  },
  "scripts": {
    "start": "REACT_APP_VIDISPINE_URL=${VIDISPINE_URL} REACT_APP_GIT_COMMIT=${npm_package_version} react-scripts start",
    "build": "GENERATE_SOURCEMAP=false REACT_APP_GIT_COMMIT=${npm_package_version} react-scripts build && cp build/index.html build/404.html",
    "predocker:build": "PUBLIC_URL=/ yarn build",
    "docker:build": "docker build -t \"${npm_package_name:1}:latest\" -t \"${npm_package_name:1}:${npm_package_version}\" .",
    "docker:push": "docker push \"${npm_package_name:1}:${npm_package_version}\" && docker push \"${npm_package_name:1}:latest\"",
    "docker:run": "docker run --name \"${npm_package_name/@vidispine\\//vidispine_}\" --rm -dti -e VIDISPINE_URL=$VIDISPINE_URL -p 80:80 \"${npm_package_name:1}:${npm_package_version}\"",
    "docker:stop": "docker stop \"${npm_package_name/@vidispine\\//vidispine_}\"",
    "predeploy": "PUBLIC_URL=${npm_package_homepage} yarn build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "eslint": "7.11.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.2.3",
    "http-proxy-middleware": "^1.1.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
